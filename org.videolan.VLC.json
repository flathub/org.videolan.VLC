{
  "app-id": "org.videolan.VLC",
  "runtime": "org.kde.Platform",
  "runtime-version": "5.11",
  "sdk": "org.kde.Sdk",
  "command": "vlc",
  "finish-args": [
    "--share=ipc",
    "--socket=x11",
    "--socket=pulseaudio",
    "--share=network",
    "--filesystem=host",
    "--talk-name=org.freedesktop.ScreenSaver",
    "--talk-name=org.freedesktop.secrets",
    "--talk-name=org.kde.kwalletd5",
    "--talk-name=org.kde.kwalletd",
    "--talk-name=org.mpris.MediaPlayer2.Player",
    "--own-name=org.mpris.MediaPlayer2.vlc",
    "--device=all",
    "--filesystem=xdg-run/dconf",
    "--filesystem=~/.config/dconf:ro",
    "--talk-name=ca.desrt.dconf",
    "--env=DCONF_USER_CONFIG_DIR=.config/dconf"
  ],
  "rename-appdata-file": "vlc.appdata.xml",
  "rename-desktop-file": "vlc.desktop",
  "rename-icon": "vlc",
  "copy-icon": true,
  "cleanup": [
    "/include",
    "/lib/pkgconfig",
    "/share/aclocal",
    "/share/doc",
    "/share/gtk-doc",
    "/share/info",
    "/share/man",
    "/share/pkgconfig",
    "/lib/cmake",
    "*.la",
    "*.a"
  ],
  "cleanup-commands": [
    "rm -f /app/lib/vlc/plugins/plugins.dat",
    "/app/lib/vlc/vlc-cache-gen /app/lib/vlc/plugins"
  ],
  "build-options": {
    "cflags": "-O2 -g -fstack-protector-strong -D_FORTIFY_SOURCE=2",
    "cxxflags": "-O2 -g -fstack-protector-strong -D_FORTIFY_SOURCE=2",
    "ldflags": "-fstack-protector-strong -Wl,-z,relro,-z,now -L/app/lib",
    "env": {
      "V": "1"
    }
  },
  "modules": [
    "shared-modules/udev/udev-175.json",
    "shared-modules/SDL/SDL-1.2.15.json",
    "shared-modules/SDL/SDL_image-1.2.12.json",

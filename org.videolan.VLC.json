{
  "app-id": "org.videolan.VLC",
  "runtime": "org.freedesktop.Platform",
  "runtime-version": "1.6",
  "sdk": "org.freedesktop.Sdk",
  "command": "vlc",
  "finish-args": [
    "--share=ipc",
    "--socket=x11",
    "--socket=pulseaudio",
    "--share=network",
    "--filesystem=host:ro",
    "--talk-name=org.freedesktop.ScreenSaver",
    "--talk-name=org.mpris.MediaPlayer2.Player",
    "--own-name=org.mpris.MediaPlayer2.vlc",
    "--device=all",
    "--filesystem=xdg-run/dconf",
    "--filesystem=~/.config/dconf:ro",
    "--talk-name=ca.desrt.dconf",
    "--env=DCONF_USER_CONFIG_DIR=.config/dconf"
  ],
  "rename-appdata-file": "vlc.appdata.xml",
  "rename-desktop-file": "vlc.desktop",
  "rename-icon": "vlc",
  "copy-icon": true,
  "cleanup": [
    "/include",
    "/lib/pkgconfig",
    "/share/aclocal",
    "/share/doc",
    "/share/gtk-doc",
    "/share/info",
    "/share/man",
    "/share/pkgconfig",
    "/lib/cmake",
    "*.la",
    "*.a"
  ],
  "cleanup-commands": [
    "rm -f /app/lib/vlc/plugins/plugins.dat",
    "/app/lib/vlc/vlc-cache-gen /app/lib/vlc/plugins"
  ],
  "build-options": {
    "cflags": "-O2 -g -fstack-protector-strong -D_FORTIFY_SOURCE=2",
    "cxxflags": "-O2 -g -fstack-protector-strong -D_FORTIFY_SOURCE=2",
    "ldflags": "-fstack-protector-strong -Wl,-z,relro,-z,now -L/app/lib",
    "env": {
      "V": "1"
    }
  },
  "modules": [
    "shared-modules/udev/udev-175.json",
    "shared-modules/SDL/SDL-1.2.15.json",
    "shared-modules/SDL/SDL_image-1.2.12.json",
    "shared-modules/lua5.3/lua-5.3.2.json",
    "shared-modules/glu/glu-9.0.0.json",
    {
      "name": "qt5-qtbase",
      "cleanup": [
        "/bin",
        "/doc",
        "/include",
        "/lib/*.a",
        "/lib/*.la",
        "/lib/*.prl",
        "/lib/cmake",
        "/lib/debug",
        "/lib/mkspecs",
        "/lib/pkgconfig"
      ],
      "config-opts": [
        "-confirm-license",
        "-opensource",
        "-debug",
        "-shared",
        "-platform",
        "linux-g++",
        "-optimized-qmake",
        "-nomake",
        "examples",
        "-nomake",
        "tests",
        "-system-harfbuzz",
        "-system-sqlite",
        "-accessibility",
        "-dbus-linked",
        "-fontconfig",
        "-glib",
        "-icu",
        "-openssl-linked",
        "-no-pch",
        "-no-rpath",
        "-no-separate-debug-info",
        "-no-directfb",
        "-no-linuxfb",
        "-no-kms",
        "-no-cups",
        "-system-proxies",
        "-archdatadir",
        "/app/lib/"
      ],
      "sources": [
        {
          "type": "archive",
          "url": "https://download.qt.io/official_releases/qt/5.9/5.9.3/submodules/qtbase-opensource-src-5.9.3.tar.xz",
          "sha256": "9e7c44005e7691dc7c85165bd4510282c47f0163521f4973eab71dbdb39a9982"
        },
        {
          "type": "shell",
          "commands": [
            "mv configure configure.qt"
          ]
        },
        {
          "type": "script",
          "commands": [
            "processed=`sed -e 's/--/-/g ; s/=/ /g' <<< $@`",
            "./configure.qt $processed"
          ],
          "dest-filename": "configure"
        },
        {
          "type": "patch",
          "path": "qtbase-hidpi_scale_at_192.patch"
        }
      ]
